<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vk.notesapp">

    <application
        android:name="com.vk.notesapp.NoteApp"
        android:allowBackup="true"
        android:icon="@drawable/baseline_camera_24"
        android:label="@string/app_name"
        android:roundIcon="@drawable/baseline_camera_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.NoteApp">
        <activity
            android:name="com.vk.notesapp.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.NoteApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>